{
  "manifest_version": 3,
  "name": "HoverTranslate for YouTube",
  "version": "1.0",
  "default_locale": "en",
  "description": "__MSG_description__",
  "key": "",
  "permissions": ["activeTab", "storage", "scripting", "identity"],
  "oauth2": {
    "client_id": "67431023275-3nresa6s25t492krt3u8b3uvvqc0ujkb.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile",
      "openid"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["*://*.youtube.com/*"],
      "js": ["./extension/dist/content.bundle.js"],
      "css": ["./extension/src/content/styles.css"]
    }
  ],
  "background": {
    "service_worker": "./extensionV2/dist/background.bundle.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["*.*"],
        "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "./popup/dist/index.html"
  },
  "icons": {
    "16": "./assets/icons/logo_16.png",
    "32": "./assets/icons/logo_32.png",
    "48": "./assets/icons/logo_48.png",
    "128": "./assets/icons/logo_128.png"
  }
}
